        box.toggle();
        button.toggleClass('active');
    });
      box.mouseenter(function()
      {
              box.toggle();
        button.toggleClass('active');
      });
       box.mouseleave(function()
      {
              box.toggle();
        button.toggleClass('active');
      });
    form.mouseup(function() { 
