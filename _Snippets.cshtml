}

/* Login Button */
#loginButton { 
    display:inline-block;
    float:right;
    text-decoration: none;
    border-radius:3px;
    -moz-border-radius:3px;
    position:relative;
    z-index:30;
    cursor:pointer;
}

#loginButton span img{ max-width: 40px; }

#signupButton { 
    display:inline-block;
    float:right;
     
     
    border-radius:7px;
    -moz-border-radius:7px;
